    - var _width = (1 / (data.length + 0.5)) * 100; //- % of available width | 0.5 experimentally determined spacing coefficiant
    .endeavor-item.endeavor-flex-row-nowrap.endeavor-flex-around
        each datum, index in data
            +endeavorImageHeader(datum, _width)


//-  Endeavor Project | Adaptive Horizontal Column Elementz
    Requires: <data.quantity> <data.units> <data.noun>
    Description: Apaptive Horizontal Column Element for simple infographic explanations

mixin endeavorInfographic(data, _width)
    - var quantity = data.quantity
    - var units = data.units
    - var noun = data.noun
    - var bgColor = data.bgColor ? data.bgColor : "transparent"
    - var bgColor2 = data.bgColor2 ? data.bgColor2 : "transparent"
    - var textColor = data.textColor ? data.textColor : "#232323" // $BLACK
    - var textColor2 = data.textColor2 ? data.textColor2 : "#232323" // $BLACK
    .endeavor-item-all-25(style=`background-color: ${bgColor}`)
        .endeavor-square-125px.endeavor-center.endeavor-circle.endeavor-flex.endeavor-flex-center.endeavor-item-bottom-25(style=`background-color: ${bgColor2}`)
            h1.thin.endeavor-item-no-margin.endeavor-text-center(style=`color: ${textColor2}`)=quantity
        p.endeavor-item-no-margin.endeavor-text-center(style=`color: ${textColor}`).endeavor-item-bottom-15=units
        h3.endeavor-item-no-margin.endeavor-text-center(style=`color: ${textColor}`).caption.caption-mod.no-margin=noun


//-  Endeavor Project | Adaptive Horizontal Row Iterator
    Requires: Infographic Module
    Description: Apaptive, Responsive Horizontal Row Iterator for simple infographic explanations

mixin endeavorInfographicGrid(data)
    .endeavor-item-125.endeavor-flex-row-nowrap.endeavor-flex-around.endeavor-horizontal-10vw
        each datum, index in data
            +endeavorInfographic(datum)


//-  Endeavor Project | Adaptive Horizontal Row Iterator
    Requires: Infographic Module
    Description: Apaptive, Responsive Horizontal Row Iterator for simple infographic explanations

mixin endeavorInfographicCompare(data)
    - var bgColor = data.bgColor ? data.bgColor : "transparent"
    - var textColor = data.textColor ? data.textColor : "#232323" // $BLACK
    - var label = data.label ? data.label : ""
    - var contents = data.contents
    .endeavor-flex.endeavor-horizontal-10vw.endeavor-item-no-margin
        .endeavor-side-label.endeavor-flex.endeavor-flex-center(style=`background-color: ${bgColor}`)
            h3.endeavor-rot-90-ccw(style=`color: ${textColor}`)=label
        .endeavor-flex-row-nowrap.endeavor-flex-around.endeavor-flex-center-main.endeavor-flex-2
            each content, index in contents
                +endeavorInfographic(content)


//-  Endeavor Project | Full 100vw Element
    Requires: <data.modules> <data.modules.data> 
    Description: Useful for creating full screen extensive backgrounds + overlays

mixin endeavorModuleMachine(data)
    - var bgColor = data.bgColor ? data.bgColor : ""
    - var _class = data.class ? data.class : "endeavor-item"
    .endeavor-width-100vw.endeavor-bg-200.endeavor-bg-center.endeavor-pos-rel.endeavor-flex-column.endeavor-flex-center-main(class=`${_class}`)(style=`background-color: ${bgColor}`)
        .endeavor-item-no-margin.endeavor-width-norm
            each module, index in data.modules
                +#{module.name}(module.data)





































