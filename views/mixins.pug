//-  Case-Study Preview
    Requires: <data> <index> 
        + route <scr/string>
        + name-preview <string>
        + project-type <string>
        + project-name <string> 
        + project-description <string>
        + project-image-preview <src>

mixin caseStudyPreview(data, index)
    .case-study.case-study-window.item.case-study-item.endeavor-height-1000px-responsive.endeavor-height-600px-mobile&attributes( {'data-inproject': false}, {'data-projectIndex': index}, {'data-project': data.route},  {'id': data.preview} )
        .case-study-overview.endeavor-overview.endeavor-overview-preview
            .description
                .description-wrapper
                    h3=data.project_type
                    h2.project-intro=data.name
                    hr
                    //- p=data.description.quote
            .hero-box
                .hero(style=`background-image: url(${data.image_hero})`)


//-  Case-Study Preview Alt
    Requires: <data> <index> 
        + route <scr/string>
        + name-preview <string>
        + project-type <string>
        + project-name <string>
        + project-description <string>
        + project-image-preview <src>

mixin caseStudyPreviewAlt(data, index)
    .case-study.case-study-window.item.case-study-item.endeavor-height-1000px-responsive.endeavor-height-600px-mobile&attributes( {'data-inproject': false}, {'data-projectIndex': index}, {'data-project': data.route},  {'id': data.preview} )
        .case-study-overview.endeavor-overview.endeavor-overview-preview.endeavor-flex-row
            .description-preview.endeavor-flex-column.endeavor-flex-start2
                .description-wrapper.endeavor-flex-column.endeavor-flex-align-start
                    h3=data.project_type
                    h2.project-intro=data.name
            .hero-box
                .hero(style=`background-image: url(${data.image_hero})`)


//-  Project-Study Preview
    Requires: <data> <index>
        + route <scr/string>
        + name-preview <string>
        + project-name <string>
        + project-description <string>
        + project-image-preview <src>

mixin projectPreview(data, index)
    li.select-work-item.item.project-item&attributes( {'data-inproject': false}, {'data-projectIndex': index}, {'data-project': data.route}, {'id': data.preview} )
        .select-work-img-container
            .select-work-description
                p.tamer=data.description.quote
                hr
            .select-work-img(style=`background-image: url(${data.image_preview})`)
        h3 #{data.name}


//-  Endeavor Header
    Requires: <data>
        + route <scr/string>
        + name-preview <string>
        + project-name <string>
        + project-description <string>
        + project-image-preview <src>

mixin endeavorHeader(data)
    .endeavor-overview
        .description
            .description-wrapper
                h3=data.project_type
                h2.project-intro=data.name
                hr
                //- p=data.description.brief
        .hero-box.hero-parallax.parallax__group
            .hero.parallax__layer--back.no-hover(style=`background-image: url(${data.image_hero})`)


//-  Endeavor Main Description & Meta Info
    Requires: <data.description> <data.meta>
        + project-description <{string}>
        + project-meta <{string}>
            + Client
            + Year
            + Role
            + Team
            + Discipline
            + Medium
            + Technologies
            + Duration

mixin endeavorIntro(data)
    - var description = data.description
    - var meta = data.meta
    .endeavor-intro.cf
        .endeavor-description 
            q=description.quote
            h4 Brief
            p.tamer=description.brief
            h4 Challenge
            p.tamer=description.challenge
            h4 Solution
            p.tamer=description.solution
        .meta-data
            ul
                li Client
                li=meta.Client
            ul
                li Year
                li=meta.Year
            ul
                li Role
                li=meta.Role
            ul
                li Team
                li=meta.Team
            ul
                li Discipline
                li=meta.Discipline
            ul
                li Medium
                li=meta.Medium
            ul
                li Technologies
                li=meta.Technologies
            ul
                li Duration
                li=meta.Duration


//-  Endeavor Animated Ghost Button
    Requires: <data.text><data.id> --> For Js access
    Description: Full-width element with centered button

mixin endeavorGhostButton(data)
    - var text = data.text ? data.text : "666"
    - var _id = data.id ? data.id : "666"
    - var _class = data.class ? data.class : "endeavor-item-125"
    - var textColor = data.textColor ? data.textColor : "#232323" // $BLACK
    .fsw.endeavor-text-center(class=`${_class}`)
        button(id=`${_id}`).endeavor-button.ghost.endeavor-center
            h3.endeavor-center.endeavor-text-center.endeavor-transition-250(style=`color: ${textColor}`)=text


//-  Endeavor Design Process Section
    Requires: <data.process><data.number><data.text>
    Description: Full-width element with centered button

mixin endeavorDesignProcess(data)
    - var text = data.text ? data.text : "666"
    - var process = data.process ? data.process : "666"
    - var number = data.number ? data.number : "666"
    - var _class = data.class ? data.class : "endeavor-item-125"
    - var textColor = data.textColor ? data.textColor : "#232323" // $BLACK
    - var textColor2 = data.textColor2 ? data.textColor2 : "#232323" // $BLACK
    .endeavor-pos-rel.endeavor-width-100vw.endeavor-height-750px.endeavor-height-500px-1000.endeavor-bg-graydient(class=`${_class}`)
        .endeavor-width-norm.endeavor-flex.endeavor-flex-center.endeavor-center.endeavor-height-100-prcnt
            h3.endeavor-top-left.endeavor-padding-top-50(style=`color: ${textColor}`) Design Process
            h3.endeavor-top-right.endeavor-padding-top-50(style=`color: ${textColor}`) D5
            div
                h1.endeavor-section-opener.endeavor-text-center.endeavor-no-padding(style=`color: ${textColor2}`)=process
                h3.endeavor-text-center(style=`color: ${textColor}`)=number
        .endeavor-width-norm.endeavor-max-width-750px.endeavor-flex.endeavor-flex-center.endeavor-center.endeavor-height-0.endeavor-no-flow
            p.endeavor-block.endeavor-text-center(style=`color: ${textColor}`)=data.text


//-  Endeavor Site Content Section
    Requires: <data.sectionName><data.number>
    Description: Full-width element with centered button

mixin endeavorSectionOpener(section, number, _class_)
    - var section = section ? section : "666"
    - var number = number ? number : "666"
    - var _class = _class_ ? _class_ : "endeavor-item-no-margin"
    - var textColor  = "#bfbfbf" // $BLACK
    - var textColor2 = "#d9d9d9" // $BLACK
    .endeavor-pos-rel.endeavor-flex-self-center.endeavor-width-100-prcnt.endeavor-height-750px.endeavor-height-500px-1000.endeavor-bg-graydient(class=`${_class}`)
        .endeavor-width-norm.endeavor-flex.endeavor-flex-center.endeavor-center.endeavor-height-100-prcnt
            h3.endeavor-top-left.endeavor-padding-top-50(style=`color: ${textColor}`) Design Section
            h3.endeavor-top-right.endeavor-padding-top-50(style=`color: ${textColor}`) C3
            div
                h1.endeavor-section-opener.endeavor-text-center.endeavor-no-padding(style=`color: ${textColor2}`)=section
                h3.endeavor-text-center(style=`color: ${textColor}`)=number


